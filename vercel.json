{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@godsreveal/vercel-bun@0.2.4"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "RUNTIME": "bunjs"
  }
}
