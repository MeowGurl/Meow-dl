{
  "version": 2,
  "builds": [
    { "src": "scripts/downloader.ts", "use": "@vercel/node" },
    { "src": "public/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/scripts/downloader.ts" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "env": {
    "RUNTIME": "bunjs"
  }
}
